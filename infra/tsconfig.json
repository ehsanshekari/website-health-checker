{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "../node_modules/@types"
    ],
    "types": [
      "node",
      "jest"
    ],
    "isolatedModules": true,
    "rootDir": "src",
    "outDir": "dist"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "cdk.out",
    "dist"
  ]
}
